{
  "name": "notivuecation",
  "version": "3.1.1",
  "description": "Promise-based alert, confirm and other notifications for vue.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepublishOnly": "npm-run-all -s validate build",
    "validate": "npm-run-all -p lint test",
    "dev": "npm-run-all -p dev:*",
    "dev:babel": "babel ./src -x \".ts\" --out-dir ./ --watch",
    "dev:ts": "tsc --noEmit --allowJs --watch",
    "build": "npm-run-all -s clean webpack-prod-build",
    "build:babel": "babel ./src -x \".ts\" -x \".js\" --out-dir ./",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "build:copyvue": "shx cp ./src/lib/*.vue lib",
    "test": "cross-env NODE_ENV=test nyc --all mocha \"./test/**/*.spec.{ts,js}\"",
    "test:dev": "mocha -w --watch-extensions ts,js \"./test/**/*.spec.{ts,js}\"",
    "clean": "npm-run-all clean:*",
    "clean:test": "shx rm -rf coverage .nyc_output",
    "clean:npm": "shx rm -rf lib tmp index.js",
    "doc": "npm-run-all -p doc:*",
    "doc:typedoc": "typedoc --out docs/ src/ --mode file",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js --cache",
    "lint:ts": "tslint src/**/*.ts -c tslint.json -p tsconfig.json -t verbose",
    "prettify": "prettier --write \"src/**/*.{js,ts,json}\"",
    "precommit": "lint-staged",
    "webpack-prod-build": "webpack --config ./webpack.config.js --mode production --progress"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,json}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "npm run lint:js"
      ],
      "src/**/*.ts": [
        "npm run lint:ts"
      ]
    }
  },
  "pre-push": [
    "validate"
  ],
  "author": "Peter van der Noord",
  "license": "MIT",
  "keywords": [
    "notification",
    "dialog",
    "modal",
    "alert",
    "confirm",
    "vue"
  ],
  "bugs": {
    "url": "https://github.com/petervdn/notivuecation/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petervdn/notivuecation.git"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.53",
    "@babel/core": "7.0.0-beta.53",
    "@babel/plugin-transform-runtime": "7.0.0-beta.53",
    "@babel/preset-env": "7.0.0-beta.53",
    "@babel/preset-stage-3": "7.0.0-beta.53",
    "@babel/preset-typescript": "7.0.0-beta.53",
    "@babel/register": "7.0.0-beta.53",
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^4.1.2",
    "coveralls": "^2.11.6",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.4.0",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "path": "^0.12.7",
    "prettier": "^1.9.2",
    "shx": "^0.2.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.53"
  }
}
